{
  "name": "mta-project",
  "version": "0.0.1",
  "description": "Build and deployment scripts",
  "scripts": {
    "clean": "rimraf resources mta_archives mta-op*",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/AppDesti_0.0.1.mtar -f --delete-services",
    "undeploy": "cf undeploy AppDesti --delete-services --delete-service-keys --delete-service-brokers"
  },
  "devDependencies": {
    "@sap/ui5-builder-webide-extension": "^1.1.9",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  }
}
